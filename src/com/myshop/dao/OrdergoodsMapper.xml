<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.myshop.dao.OrdergoodsMapper">
	<insert id="insertOrdergoods" parameterType="List">
		insert into ordergoods(name, price, num, orderId)
		VALUES
		<foreach collection="OrdergoodsList" item="item" index="index" separator=",">
			(#{item.name},#{item.price},#{item.num},#{item.orderId})
		</foreach>
	</insert>
</mapper>